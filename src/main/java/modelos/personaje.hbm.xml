<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="modelos.Personaje" table="personaje">
		<id column="idPersonaje" name="idPersonaje" type="int">
              <generator class="increment" />
		</id>
		<property column="idInventario" name="idInventario" type="int" />
		<property column="idMochila" name="idMochila" type="int" />
		<property column="casta" name="casta" type="string" />
		<property column="raza" name="raza" type="string" />
		<property column="fuerza" name="fuerza" type="int" />
		<property column="destreza" name="destreza" type="int" />
		<property column="inteligencia" name="inteligencia" type="int" />
		<property column="saludTope" name="saludTope" type="int" />
		<property column="energiaTope" name="energiaTope" type="int" />
		<property column="nombre" name="nombre" type="string" />
		<property column="experiencia" name="experiencia" type="int" />
		<property column="nivel" name="nivel" type="int" />
		<property column="idAlianza" name="idAlianza" type="int" />
		<property column="invulnerable" name="invulnerable" type="int" />
		<property column="dobleFuerza" name="dobleFuerza" type="int" />
		<property column="mitadFuerza" name="mitadFuerza" type="int" />
		<property column="invisible" name="invisible" type="int" />
		<property column="atravesarParedes" name="atravesarParedes" type="int" />
		<property column="ptosAsigFuerza" name="ptosAsigFuerza" type="int" />
		<property column="ptosAsigDestreza" name="ptosAsigDestreza" type="int" />
		<property column="ptosAsigInteligencia" name="ptosAsigInteligencia" type="int" />
	</class>
</hibernate-mapping>
